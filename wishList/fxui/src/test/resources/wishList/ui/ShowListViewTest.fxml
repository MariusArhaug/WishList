<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="showListPane" prefHeight="400.0" prefWidth="298.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wishList.ui.ShowListViewController">
   <children>
      <Button id="addNewWishButton" fx:id="addNewWishButton" layoutX="14.0" layoutY="22.0" mnemonicParsing="false" onAction="#addWish" text="New wish" />
      <Button id="signOut" fx:id="signOut" layoutX="14.0" layoutY="47.0" mnemonicParsing="false" onAction="#signOut" text="Sign out" />
      <Button id="backToYourWishLists" fx:id="backToYourWishLists" layoutX="14.0" layoutY="72.0" mnemonicParsing="false" onAction="#changeToMainView" text="&lt;-" />
      <Button id="removeWish" fx:id="removeWish" layoutX="14.0" layoutY="97.0" mnemonicParsing="false" onAction="#removeWish" text="Remove wish" />
      <ListView id="wishesListView" fx:id="wishesListView" layoutX="6.0" layoutY="200.0" prefHeight="200.0" prefWidth="200.0" />
      <TextField id="addNewWishField" fx:id="addNewWishField" layoutX="14.0" layoutY="131.0" />
      <Button fx:id="yourFriends" layoutX="206.0" layoutY="60.0" mnemonicParsing="false" onAction="#changeToFriendsView" text="Friends" />
      <Button id="friendWishLists" fx:id="friendWishLists" layoutX="206.0" layoutY="97.0" mnemonicParsing="false" onAction="#changeToFriendsWishListsView" text="Friend's list" />
      <Label id="addWishFeedback" fx:id="addWishFeedback" layoutX="192.0" layoutY="26.0" />
      <Button id="shareList" fx:id="shareList" layoutX="206.0" layoutY="22.0" mnemonicParsing="false" onAction="#changeToGroupsView" text="Share" />
   </children>
</AnchorPane>
