<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="257.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wishList.ui.MainViewController">
   <children>
      <Button id="addNewList" fx:id="addNewList" layoutX="22.0" layoutY="31.0" mnemonicParsing="false" onAction="#makeNewWishList" text="New list" />
      <Button id="enterList" fx:id="enterList" layoutX="22.0" layoutY="56.0" mnemonicParsing="false" onAction="#enterWishList" text="Enter list" />
      <Button id="removeWishList" fx:id="removeWishList" layoutX="22.0" layoutY="81.0" mnemonicParsing="false" onAction="#removeWishList" text="Remove list" />
      <Button id="signOut" fx:id="signOut" layoutX="22.0" layoutY="106.0" mnemonicParsing="false" onAction="#signOut" text="Sign out" />
      <Label id="newWishListFeedback" fx:id="newWishListFeedback" layoutX="22.0" layoutY="146.0" prefHeight="17.0" prefWidth="49.0" text="" />
      <TextField id="addNewListField" fx:id="addNewListField" layoutX="14.0" layoutY="175.0" />
      <ListView id="list" fx:id="list" layoutX="14.0" layoutY="243.0" prefHeight="157.0" prefWidth="165.0" />
      <Button id="yourFriends" fx:id="yourFriends" layoutX="163.0" layoutY="31.0" mnemonicParsing="false" onAction="#changeToFriendsView" text="Friends" />
      <Button id="friendWishLists" fx:id="friendWishLists" layoutX="165.0" layoutY="69.0" mnemonicParsing="false" onAction="#changeToFriendsWishListsView" text="Friends list" />
   </children>
</AnchorPane>
